{
  "version": 2,
  "functions": {
    "api/send-dana-data.js": {
      "maxDuration": 10,
      "memory": 256
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
